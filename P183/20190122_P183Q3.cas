P183Q3	START			;((3x10)+5)x10
	RPUSH
	LD	GR1,=3		;かけられる数
	LD	GR2,=10		;かける数
	LD	GR3,=5		;足す数
	CALL	MULT		;x10
	ADDA	GR1,GR3		;+5
	CALL	MULT		;x10
	RPOP
	RET
;
MULT	PUSH	0,GR2
	PUSH	0,GR7
	LD	GR7,GR1
	LD	GR1,=0		;結果用レジスタクリア
LOOP	CPA	GR2,=0
	JZE	FIN
	ADDA	GR1,GR7
	LAD	GR2,-1,GR2
	JUMP	LOOP
FIN	POP	GR7
	POP	GR2
	RET
;
	END
