P183Q9  START
	RPUSH
    	LAD	GR1,DATA
    	LD	GR2,LEN
	OUT	DATA,LEN
	CALL	REV
	OUT	DATA,LEN
    	RPOP
    	RET
; 副プログラム
REV	RPUSH
	LD	GR4,GR1
	ADDA	GR4,GR2		; 最終番地
LOOP1	LD	GR3,0,GR1	; 文字読み込み
	CPA	GR1,GR4		; 最終番地到達
	JZE	REV2
	PUSH	0,GR3		; スタック
	LAD	GR1,1,GR1	; アドレス+1
	JUMP	LOOP1
;
REV2	LAD	GR1,DATA
LOOP2	CPA	GR1,GR4		; 最終番地到達
	JZE	FIN
	POP	GR3		; スタック取り出し
	ST	GR3,0,GR1	; DATAに格納
	LAD	GR1,1,GR1	; アドレス+1
	JUMP	LOOP2
FIN	RPOP
	RET
;
DATA	DC	'ABCDEFG'
LEN	DC	7
    	END